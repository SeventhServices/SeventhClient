<Project ToolsVersion="15.0">
  <Import Project="version.props" />

  <PropertyGroup Label="Source Link">
    <ProjectParentDirectory>$([System.IO.Path]::GetDirectoryName('$(MSBuildProjectDirectory)'))</ProjectParentDirectory>
    <ProjectParentDirectoryName>$([System.IO.Path]::GetFileName('$(ProjectParentDirectory)'))</ProjectParentDirectoryName>
  </PropertyGroup>

  <PropertyGroup Label="Package">
    <RepositoryUrl>https://github.com/SeventhServices/SeventhClient</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Product>SeventhServices</Product>
    <Company>Sagilio</Company>
    <Authors>Sagilio</Authors>
    <!--<GenerateDocumentationFile>true</GenerateDocumentationFile>-->

    <PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/SeventhServices/Seventh.Client</PackageProjectUrl>

  </PropertyGroup>
</Project>